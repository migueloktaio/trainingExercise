    <validationRules>
        <fullName>Copies_not_available</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL(Book_Copy__r.Availability__c, &quot;Not Available&quot;), OwnerId &lt;&gt; student__r.Id )</errorConditionFormula>
        <errorDisplayField>Book_Copy__c</errorDisplayField>
        <errorMessage>There are no copies available for this book</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Return_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Loan_Date__c &gt;=  Expected_Return_Day__c</errorConditionFormula>
        <errorDisplayField>Expected_Return_Day__c</errorDisplayField>
        <errorMessage>Return day should be after the date of Loan</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Student_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( student__c)</errorConditionFormula>
        <errorDisplayField>student__c</errorDisplayField>
        <errorMessage>Student should not be in blank</errorMessage>
    </validationRules>
